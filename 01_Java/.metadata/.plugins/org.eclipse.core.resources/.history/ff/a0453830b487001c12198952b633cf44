package com.mystudy.array;

public class Ex02_array2 {

	public static void main(String[] args) {
		// 1~10까지의 수를 배열에 저장(저장할때 반복문사용)
		
		int[] qwer = new int[10];
		
		for (int i = 0; i < qwer.length; i++) {
			qwer[i] = 1 + i;
			System.out.println("qwer[" + i + "] : " + qwer[i]);
		}
		
		
		// 1~10 까지의 수중 홀수의 합 (배열)
		// 저장공간(odd) 5개 만 있다 해서 먼저 홀수값을 구하고 배열에 넣기(반복문 사용)
		// 마지막으로 배열에 저장된 값을 모두 합하여 출력
		
		
		int[] odd = new int[5];
		int k = 0;
		
		for (int i = 1; i <= 10; i++) {
			if (i % 2 == 1) {
				odd[k] = i;
				}
			
		}
		System.out.println(odd[0]);
		System.out.println(odd[1]);
		System.out.println(odd[2]);
		System.out.println(odd[3]);
		System.out.println(odd[4]);
		
		

	}

}
