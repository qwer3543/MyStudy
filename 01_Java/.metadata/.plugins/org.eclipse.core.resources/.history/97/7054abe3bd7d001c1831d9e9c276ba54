
public class Ex03 {

	public static void main(String[] args) {
		// 10진수, 2진수, 8진수, 16진수
		int x = 10; // 10진수
		int y = 0b1010; // 2진수(0B로 시작시 2진수를 뜻함)
		int z = 012;  // 8진수(0으로 시작시 8진수를 뜻함)
		
		
		System.out.println("x = "+x);
		System.out.println("y = "+y);
		System.out.println("z = "+z);
		

	}

}
