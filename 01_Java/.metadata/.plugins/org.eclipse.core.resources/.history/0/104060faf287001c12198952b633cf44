package com.mystudy.array3_copy;

public class ArrayCopy {

	public static void main(String[] args) {
		// 배열의 복사 
		
		int[] num1 = new int[5];
		//int[] num2 = new int[5];
		int[] num2 = num1;
		
		System.out.println("num1: " + num1);
		System.out.println("num2: " + num2);
		System.out.println("num1 == num2 " + (num1 == num2));
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
